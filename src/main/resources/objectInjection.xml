<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="lop" class="vn.edu.iuh.fit.objectInjection.ClassObject">
        <constructor-arg index="0" value=""/>
        <constructor-arg index="1" value=""/>
        <property name="classId" value="DHTH15A"/>
        <property name="className" value="Lop DHTH15A"/>
    </bean>


    <bean id="st1" class="vn.edu.iuh.fit.objectInjection.StudentObject">
        <property name="aClass" ref="lop"/>
        <property name="stuName" value="Than Thi Det"/>
        <property name="stuId" value="1001"/>
    </bean>

</beans>